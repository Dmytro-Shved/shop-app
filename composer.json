{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "afatmustafa/blade-hugeicons": "^0.0.3",
        "anayarojo/shoppingcart": "^4.2",
        "andreiio/blade-remix-icon": "^3.5",
        "blade-ui-kit/blade-heroicons": "^2.5",
        "blade-ui-kit/blade-zondicons": "^1.5",
        "codeat3/blade-carbon-icons": "^2.31",
        "codeat3/blade-clarity-icons": "^1.9",
        "codeat3/blade-eos-icons": "^1.14",
        "codeat3/blade-fluentui-system-icons": "^1.33",
        "codeat3/blade-google-material-design-icons": "^1.19",
        "codeat3/blade-iconpark": "^1.6",
        "codeat3/blade-mono-icons": "^1.4",
        "codeat3/blade-phosphor-icons": "^2.2",
        "davidhsianturi/blade-bootstrap-icons": "^1.5",
        "faisal50x/blade-ionicons": "^1.0",
        "hasnayeen/blade-eva-icons": "^2.0",
        "laravel/framework": "^11.9",
        "laravel/tinker": "^2.9",
        "livewire/livewire": "^3.5",
        "livewire/volt": "^1.6",
        "mansoor/blade-lets-icons": "^1.0",
        "owenvoke/blade-fontawesome": "^2.8",
        "postare/blade-mdi": "^1.1",
        "ryangjchandler/blade-tabler-icons": "^2.3",
        "symfony/mailer": "~7.1.0",
        "troccoli/blade-health-icons": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
